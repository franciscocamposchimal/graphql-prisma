type Book {
  id: ID! @id
  title: String!
  author: String!
  isbn: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type User {
  id: ID! @id
  username: String! @unique
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}

type Review {
  id: ID! @id
  text: String!
  rating: String!
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}